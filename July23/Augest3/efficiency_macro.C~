{

TFile *f = new TFile("output2.root","UPDATE");
TH1F *denom = new TH1F("denominator","",300,0,300);
denom->Add(NCharged,NMissing);

TH1F *hist1 = new TH1F("eff_with_OA","",300,0,300);
hist1->Divide(NChargedOA,denom,1.,1.,"b");
hist1->SetTitle("Rootino Detection Efficiency- with OA cut = 15 deg");
hist1->GetXaxis()->SetTitle("Missing Particle Kinetic Energy (MeV)"); 
hist1->Draw();
//TFile *f2 = new TFile("output.root","UPDATE");
hist1->Write();
f.Close();
}


